{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
      "config:base",
      ":dependencyDashboard",
      ":disableRateLimiting",
      "docker:enableMajor",
      "docker:pinDigests",
      ":automergeDigest",
      ":enablePreCommit",
      "github>kilip/renovate-config:commit-message",
      "github>kilip/renovate-config:pr-labels",
      "github>kilip/renovate-config:semantic-commits",
      "github>kilip/renovate-config:generic-regex-manager"
    ],
    "onboarding": false,
    "requireConfig": "optional",
    "timezone": "Asia/Makassar",
    "dependencyDashboardTitle": "Renovate Dashboard",
    "suppressNotifications": ["prIgnoreNotification"],
    "commitBodyTable": true,
    "rebaseWhen": "conflicted"
  }